<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.matferib.Tabuleiro"
      android:versionCode="9"
      android:versionName="1.7.1">
    <application android:label="@string/app_name" android:icon="@drawable/icone_tabuleiro" android:debuggable="true">
        <activity android:name=".SelecaoActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TabuleiroActivity"
                  android:label="@string/app_name"
                  android:configChanges="orientation|screenSize|keyboard|keyboardHidden"
                  android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-sdk android:minSdkVersion="19" />
</manifest>
