    # A arma se torna primaria com ataques multiplos e a arma natural vira secundaria.
    dados_ataque {
      grupo: 'ataque_total_espada' rotulo: 'espada_larga' tipo_ataque: 'Ataque Corpo a Corpo' empunhadura: EA_MONSTRO_ATAQUE_PRIMARIO id_arma: 'espada_larga' ordem_ataque: 0
